    "uuid": "^9.0.1",
    "xmlrpc": "^1.3.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.3"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.10.5",
    "@types/table": "^6.3.2",
    "@types/unzipper": "^0.10.9",
    "@types/uuid": "^9.0.7",
    "@types/xmlrpc": "^1.3.10",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
